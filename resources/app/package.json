{
    "name": "CComputer",
    "version": "1.7.11",
    "description": "CComputer",
    "main": "index.js",
    "scripts": {
        "build_win_x64": "electron-packager . --platform=win32 --arch=x64 --electron-version=1.7.11 --overwrite --ignore=node_modules --ignore=.gitignore",
        "build_win_ia32": "electron-packager . --platform=win32 --arch=ia32 --electron-version=1.7.11 --overwrite --ignore=node_modules --ignore=.gitignore",
        "build_linux_x64": "electron-packager . --platform=linux --arch=x64 --electron-version=1.7.11 --overwrite --ignore=node_modules --ignore=.gitignore",
        "build_linux_ia32": "electron-packager . --platform=linux --arch=ia32 --electron-version=1.7.11 --overwrite --ignore=node_modules --ignore=.gitignore",
        "package": "asar pack CComputer/resources/app CComputer/resources/app.asar && del CComputer/resources/app"
    },
    "dependencies": {
        "electron": "^1.8.4",
        "electron-packager": "^10.1.2",
        "fft-js": "0.0.11",
        "fft.js": "^4.0.3",
        "gpu.js": "^1.2.0",
        "keyboardjs": "^2.4.1",
        "pixi-keyboard": "^0.9.4",
        "pixi-sound": "^2.0.2",
        "pixi.js": "^4.7.1",
        "pizzicato": "^0.6.3",
        "screenfull": "^3.3.2"
    },
    "devDependencies": {
        "babel-preset-electron": "^1.4.15",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2016": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1"
    },
    "presets": [
        "es2015",
        "react",
        "stage-2",
        "env"
    ],
    "plugins": []
}
